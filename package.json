  "bugs": {
    "url": "git@github.com:BitGo/eth2-bls-wasm/issues"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "homepage": "git@github.com:BitGo/eth2-bls-wasm#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@chainsafe/babel-plugin-inline-binary-import": "^1.0.3",
    "assert": "^2.0.0",
